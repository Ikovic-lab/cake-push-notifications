{
    "name": "scherersoftware/cake-push-notifications",
    "description": "Push Notification plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "cakephp/plugin-installer": "*",
        "zendframework/zendservice-google-gcm": "dev-master",
        "zendframework/zendservice-apple-apns": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Scherersoftware\\CakePushNotifications\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakePushNotifications\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
